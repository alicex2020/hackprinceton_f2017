<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gymder.MatchPage">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/Match1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Yash is free at 6pm"
            android:textSize="30sp"/>


        <TextView
            android:id="@+id/MisMatch1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Khyati is not free today"
            android:textSize="30sp"/>

        <TextView
            android:id="@+id/MisMatch2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Alice is not free today"
            android:textSize="30sp"/>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
