<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.quickstart.Match2"
    tools:showIn="@layout/activity_match2">

    <TextView
        android:id="@+id/Match1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="86dp"
        android:background="@drawable/my_border"
        android:text="Yash is free at 6pm"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/MisMatch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Match1"
        android:layout_marginTop="81dp"
        android:background="@drawable/my_border"
        android:text="Khyati is not free today"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/MisMatch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Match1"
        android:layout_marginTop="193dp"
        android:text="Alice is not free today"
        android:textSize="30sp"
        android:background="@drawable/my_border"
        />

</RelativeLayout>
